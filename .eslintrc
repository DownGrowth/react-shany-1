{
    "extends": "@antfu",
    "rules": {
        "curly": "off",
        "@typescript-eslint/brace-style": "off",
        "max-statements-per-line":"off",
        "no-console":"off",
        "@typescript-eslint/no-unused-vars": [
          "error",
          {
            "vars": "local",
            "args": "none",
            "ignoreRestSiblings": true,
            "caughtErrors": "none"
          }
        ],
        "@typescript-eslint/comma-dangle": "off"
    }
}